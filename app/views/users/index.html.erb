<div class="container">
  <div class="row">
      <% @users.each do |user| %>
          <div class="col-md-4 d-flex">
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="..." alt="Card image cap">
                <div class="card-body flex-fill">
                  <h5 class="card-title"><%= user.name %></h5>
                  <h3>subdomain</h3>
                  <p class="card-text"><%= user.subdomain %></p>
                  <%= link_to 'User Profile', user_path(user), class: "btn btn-primary" %>
                </div>
            </div>
          </div>
      <% end %> 
  </div>
</div>